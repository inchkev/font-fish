{
  "name": "fontfish",
  "version": "1.0.0",
  "description": "A browser tool for spatial font discovery and exploration",
  "main": "index.js",
  "scripts": {
    "build": "esbuild js/index.js --bundle --outfile=dist/bundle.js --minify --sourcemap",
    "start": "esbuild js/index.js --bundle --outfile=dist/bundle.js --minify --sourcemap",
    "serve": "serve -s dist/",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inchkev/font-fish.git"
  },
  "keywords": [
    "font"
  ],
