  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inchkev/font-fish/issues"
  },
  "homepage": "https://github.com/inchkev/font-fish#readme",
  "dependencies": {
    "esbuild": "0.17.18",
    "three": "^0.151.3"
  }
}
